<h1>プロフィール編集</h1>
<%= form_with model: @profile do |f| %>
  <p><%= f.label :name%><%= f.text_field :name %></p>
  <p><%= f.label :description %><%= f.text_area :description %></p>
  <p><%= f.label :occupation %><%= f.text_field :occupation %></p>
  <p><%= f.file_field :image %>
  <p><%= f.submit %></p>
<% end %> 
<%= link_to 'トップページに戻る',"/posts" %>

